    "@cypress/schematic": "^2.0.3",
    "@types/jasmine": "~4.0.0",
    "cypress": "latest",
    "jasmine-core": "~4.3.0",
    "json-server": "^0.17.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2"
